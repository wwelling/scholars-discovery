PREFIX vivo: <http://vivoweb.org/ontology/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
    ?relatedBy rdfs:label ?label .
} WHERE {
    <{{uri}}> vivo:relatedBy ?relatedBy .
    ?relatedBy a vivo:AwardReceipt .
    ?relatedBy vivo:relates ?receipt .
    ?receipt rdfs:label ?label .
}
